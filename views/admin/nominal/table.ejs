<table id="example2" class="table table-bordered table-hover">
	<thead>
		<tr>
			<th>No.</th>
			<th>Coin Name</th>
			<th>Coin Quantity</th>
      <th>Price</th>
      <th>Action</th>
		</tr>
	</thead>
	<tbody>
		<% for(let i=0; i < nominal.length; i++){ %>
		<tr>
			<td><%= i+1 %></td>
			<td><%= nominal[i].coinName %></td>
			<td><%= nominal[i].coinQuantity %></td>
			<td><%= nominal[i].price %></td>
			<td>
				<form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
					<a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-info">
						<i class="nav-icon fas fa-pencil-alt"></i>
						Edit
					</a>
					<button class="btn btn-danger">
						<i class="nav-icon fas fa-trash-alt"></i>
						Delete
					</button>
				</form>
			</td>
		</tr>
		<% } %>
	</tbody>
	<tfoot>
		<tr>
			<th>No.</th>
			<th>Coin Name</th>
			<th>Coin Quantity</th>
      <th>Price</th>
      <th>Action</th>
		</tr>
	</tfoot>
</table>